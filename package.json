{
  "homepage": "https://aemooooon.github.io/hua.nz",
  "name": "hua.nz",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "// Development Commands": "Development and build-related scripts",
    "dev": "vite",
    "// ↳ Start development server with hot reload": "",
    "build": "npm run precompute && vite build",
    "// ↳ Build production version (precompute image dimensions first)": "",
    "preview": "vite preview",
    "// ↳ Preview production build locally": "",
    "// Code Quality & Formatting": "ESLint and Prettier integration",
    "lint": "eslint .",
    "// ↳ Check code quality and syntax issues": "",
    "lint:fix": "eslint . --fix",
    "// ↳ Automatically fix ESLint issues where possible": "",
    "lint:check": "eslint . --max-warnings 0",
    "// ↳ Strict linting check for CI/CD (no warnings allowed)": "",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "// ↳ Format all source files using Prettier": "",
    "format:all": "npm run format && npm run lint",
    "// ↳ Complete formatting workflow (Prettier + ESLint)": "",
    "format:check": "prettier --check \"src/**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "// ↳ Check if files are properly formatted (CI/CD usage)": "",
    "// Deployment & Utilities": "Deployment and helper scripts",
    "deploy": "gh-pages -d dist --add",
    "// ↳ Deploy to GitHub Pages": "",
    "precompute": "node scripts/precompute-image-dimensions.js",
    "// ↳ Precompute image dimensions for performance optimization": ""
  },
  "dependencies": {
    "d3": "^7.9.0",
    "d3-cloud": "^1.2.7",
